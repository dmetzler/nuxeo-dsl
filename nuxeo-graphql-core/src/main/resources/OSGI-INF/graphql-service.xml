<component name="org.nuxeo.graphql.component">


  <service>
    <provide interface="org.nuxeo.graphql.GraphQLService" />
  </service>

  <implementation class="org.nuxeo.graphql.GraphQLComponent" />


  <extension-point name="alias">
    <object class="org.nuxeo.graphql.descriptors.AliasDescriptor" />
  </extension-point>

  <extension-point name="query">
    <object class="org.nuxeo.graphql.descriptors.QueryDescriptor" />
  </extension-point>

  <extension-point name="crud">
    <object class="org.nuxeo.graphql.descriptors.CrudDescriptor" />
  </extension-point>
</component>